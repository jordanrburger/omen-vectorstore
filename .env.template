# Keboola configuration (required)
KEBOOLA_API_URL=https://connection.keboola.com
KEBOOLA_TOKEN=your-keboola-storage-api-token

# OpenAI configuration (optional - for OpenAI embedding provider)
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=text-embedding-ada-002

# Sentence Transformer configuration (optional)
SENTENCE_TRANSFORMER_MODEL=all-MiniLM-L6-v2

# Qdrant configuration
QDRANT_HOST=localhost
QDRANT_PORT=55000
QDRANT_COLLECTION=keboola_metadata 